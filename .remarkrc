{
  "settings": {
    "gfm": true,
    "yaml": true,
    "commonmark": false,
    "footnotes": true,
    "pedantic": true,
    "breaks": false,
    "entities": false,
    "setext": false,
    "closeATX": false,
    "looseTable": false,
    "spacedTable": true,
    "fence": "`",
    "fences": true,
    "bullet": "-",
    "listItemIndent": "1",
    "incrementListMarker": true,
    "rule": "-",
    "ruleRepetition": 3,
    "ruleSpaces": false,
    "strong": "_",
    "emphasis": "_",
    "position": true
  },
  "plugins": {
    "inline-links": {},
    "lint": {
      "external": ["remark-lint-code"],
      "external": ["remark-lint-no-empty-sections"],
      "blockquote-indentation": 2,
      "checkbox-character-style": {
        "checked": "x",
        "unchecked": " "
      },
      "code-block-style": "fenced",
      "emphasis-marker": "_",
      "fenced-code-flag": {
        "allowEmpty": true
      },
      "fenced-code-marker": "`",
      "file-extension": "md",
      "first-heading-level": false,
      "heading-style": "atx",
      "list-item-indent": "space",
      "list-item-spacing": false,
      "maximum-heading-length": false,
      "maximum-line-length": false,
      "no-consecutive-blank-lines": false,
      "no-html": false,
      "no-literal-urls": false,
      "ordered-list-marker-style": ".",
      "ordered-list-marker-value": "ordered",
      "rule-style": "---",
      "strong-marker": "_",
      "table-cell-padding": "padded",
      "unordered-list-marker-style": "-"
    },
    "normalize-headings": {},
    "textr": {
      "options": { "locale": "en-us" },
      "plugins": [
        "typographic-base",
        "typographic-numbers"
      ]
    }
  }
}
